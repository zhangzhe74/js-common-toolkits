{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
     "declaration": true,
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                     
    "noImplicitThis": false,                           
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": ["src"],
  "exclude": ["node_modles", "dist"]
}
